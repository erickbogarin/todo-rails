

<div class="cd-timeline-img">
</div> <!-- cd-timeline-img -->

<div class="cd-timeline-content">
	<h2><%= @todo.get('name') %><h2>
	<p><%= @todo.get('description') %></p>

	<input id="completed" type="checkbox" 
		<% if @todo.get('completed'): %>
			checked
		<% end %>	
	>
	<label for="completed">Done</label>
</div>
